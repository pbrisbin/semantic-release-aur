{
  "name": "semantic-release-aur",
  "version": "0.0.0",
  "description": "A semantic-release plugin to release packages to the AUR",
  "repository": "git@github.com:pbrisbin/semantic-release-aur.git",
  "homepage": "https://github.com/pbrisbin/semantic-release-aur",
  "bugs": "https://github.com/pbrisbin/semantic-release-aur/issues",
  "author": "Pat Brisbin <pbrisbin@gmail.com>",
  "license": "MIT",
  "keywords": [
    "semantic-release",
    "archlinux"
  ],
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "format": "prettier --write \"**/*.ts\"",
    "format-check": "prettier --check \"**/*.ts\"",
    "test": "jest --maxWorkers 1",
    "lint": "eslint"
  },
  "dependencies": {
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/semantic-release": "^20.0.6",
    "@types/signale": "^1.4.7",
    "typescript-eslint": "^8.24.1",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.21.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-sonarjs": "^3.0.2",
    "jest": "^29.7.0",
    "prettier": "^3.5.2",
    "semantic-release": "^24.2.3",
    "semantic-release-yarn": "^3.0.2",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "semantic-release": ">=22.0.0"
  },
  "peerDependenciesMeta": {
    "semantic-release": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
